pipeline {
    agent any
    environment {
        registry = "koteswarararaosairi/gotr"
        registryCredential = 'dockerhub'
        dockerImage = ''
    }
    stages {
        stage ('clone') {
            steps {
               git'https://github.com/koti73/DockerWebProject.git'
            }
        }
        stage ('image') {
            steps {
                script {
                    dockerImage = docker.build registry
                }
            }
        }
        stage ('image push to docker hub ') {
            steps {
                script {
                    docker.withRegistry ('' , registryCredential) {
                        dockerImage.push ( )
                    }
                }
            }
        }
        stage ('run conatiner') {
            steps {
                sh ''' docker pull koteswarararaosairi/gotr:latest
                docker run --name fixed2e -d -p 85:80 koteswarararaosairi/gotr:latest '''
            }
        }
    }
}
